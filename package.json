{
  "name": "ops-credential-sharing",
  "version": "1.0.4",
  "description": "A minimal Electron application to share ops credentials with other users",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": "https://github.com/mephmanx/ops-credential-sharing",
  "keywords": [
    "Electron",
    "Spring Vault",
    "GitHub",
    "cli",
    "dotcomrow"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.68",
    "@electron-forge/maker-deb": "^6.0.0-beta.68",
    "@electron-forge/maker-rpm": "^6.0.0-beta.68",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.68",
    "@electron-forge/maker-zip": "^6.0.0-beta.68",
    "@electron-forge/publisher-github": "^6.0.0-beta.68",
    "electron": "^21.2.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ops_credential_sharing"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "dotcomrow",
              "name": "ops-credential-sharing"
            },
            "authToken": "{GITHUB_TOKEN}"
          }
        }
      ]
    }
  }
}
